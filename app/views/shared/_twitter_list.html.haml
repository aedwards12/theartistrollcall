%ul
  = link_to 'View All Artists', artists_video_path(@video.id)
  %h4 Choreographer
  - if choreographer.any?
    - choreographer.each do |choreo|
      =render partial: "shared/twitter_node", locals: {twitter_node: choreo}

  %li
  %h4 Asst Choreographers
  - if assist_choreographers.any?
    - assist_choreographers.each do | twitter_node |
      =render partial: "shared/twitter_node", locals: {twitter_node: twitter_node}

  %li
  %h4 Dancers
  - if twitter_list.any?
    - twitter_list.each do | twitter_node |
      =render partial: "shared/twitter_node", locals: {twitter_node: twitter_node}